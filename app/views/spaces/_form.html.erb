

<%= form_for(@space) do |f| %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :address, "Address" %><br>
    <%= f.text_field :address %>
  </div>
  <div class="field">
    <%= f.label :neighborhood %><br>
    <%= f.text_field :neighborhood %>
  </div>
  <div class="field">
    <%= f.label :sq_footage %><br>
    <%= f.text_field :sq_footage %>
  </div>
  <div class="field">
    <%= f.label :features %><br>
    <%= f.text_area :features %>
  </div>
  <div class="field">
    <%= f.label :photo_url %><br>
    <%= f.text_field :photo_url %>
  </div>

  <div class="field">
    <%= f.label :rate %><br>
    <%= f.text_field :rate %>
  </div

  <div class="field">
    <%= f.label :contact %><br>
    <%= f.text_field :contact %>
  </div

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>