

<%= form_for Space.new do |f| %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :address, "Address" %><br>
    <%= f.text_field :address %>
  </div>
  
  <div class="field">
    <%= f.label :sq_footage %><br>
    <%= f.text_field :sq_footage %>
  </div>
  <div class="field">
    <%= f.label :features %><br>
    <%= f.text_area :features %>
  </div>
  
  <div class="field">
    <%= f.label :rate %><br>
    <%= f.text_field :rate %>
  </div>

  <div class="field">
    <%= f.label :contact %><br>
    <%= f.text_field :contact %>
  </div>

  <div class="field">
    <%= f.label :neighborhood_id %><br>
    <%= f.text_field :neighborhood_id %>
  </div>

  <div class="field">
    <%= f.label :photo_url %><br>
    <%= f.text_field :photo_url %>
  </div>

  <div class="field">
    <%= f.label :photo_ur1 %><br>
    <%= f.text_field :photo_url %>
  </div>

  <div class="field">
    <%= f.label :photo_url2 %><br>
    <%= f.text_field :photo_url1 %>
  </div>

  <div class="field">
    <%= f.label :photo_url3 %><br>
    <%= f.text_field :photo_url2 %>
  </div>
  <div class="field">
    <%= f.label :photo_url4 %><br>
    <%= f.text_field :photo_url3 %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>