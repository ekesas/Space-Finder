<%= form_for [@space, space.reviews.build] do |f| %>
  

  <div class="field">
    <%= f.label :author %><br>
    <%= f.text_field :author %>
  </div>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_field :content %>
  </div>
  <div class="field">
    <%= f.label :rating %><br>
    <%= f.text_field :rating %>
  </div>
  <div class="field">
    <%= f.label :space_id %><br>
    <%= f.text_field :space_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
