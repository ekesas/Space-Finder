


<%= form_for (@neighborhood) do |f| %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :space, "Space Name" %><br>
    <%= f.text_field :space %>
  </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>